    let imgUrl = "https://i.imgur.com/A1674e8.png";

    // Create a <style> element
    const customStyling = document.createElement("style");

    // Function to update the style based on the image URL
    function updateStyling(imgUrl) {
    customStyling.innerHTML = `
        #campaign_sign {
            background-image: url("${imgUrl}");
            background-size: cover;
            border-color: #c9c9c9;    
            border-width: .01em;
            margin: 1 auto;
            font-size: 0em;
            background-position: center;
            width: 100%; 
            height: 92px; 
            margin-left: -0.07em;
        }
        `;
    }

    // Append the <style> element to the head of the document
    document.head.appendChild(customStyling);

    // Set the initial styling
    updateStyling(imgUrl);

    // Example of changing the image URL based on a variable
    function changeImage(newImgUrl) {
    imgUrl = newImgUrl;
    console.log("Imgur Success")
    updateStyling(imgUrl);  // Update the styling with the new image URL
    }



